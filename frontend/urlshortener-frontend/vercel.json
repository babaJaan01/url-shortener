{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://urlshortener-alb-471740086.us-west-1.elb.amazonaws.com/api/$1"
    },
    {
      "source": "/:shortCode",
      "destination": "http://urlshortener-alb-471740086.us-west-1.elb.amazonaws.com/:shortCode"
    }
  ]
}
